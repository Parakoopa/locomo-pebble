	{
		"name": "Locomo",
		"version": "2.1.0",
		"description": "A Pebble watchapp that shows train times and platforms",
		"keywords": [
			"pebble-app",
			"pebble",
			"transport",
			"tfl",
			"london",
			"uk",
			"locomo",
			"commute",
			"train"
		],
		"homepage": "https://github.com/yoziru-desu/locomo-pebble/#readme",
		"license": "MIT",
		"author": "yoziru-desu",
		"repository": {
			"type": "git",
			"url": "git+https://github.com/yoziru-desu/locomo-pebble.git"
		},
		"dependencies": {
			"@smallstoneapps/data-processor": "^2.1.1",
			"@smallstoneapps/message-queue": "^2.4.2",
			"@smallstoneapps/utils": "^1.2.1"
		},
		"private": true,
		"pebble": {
